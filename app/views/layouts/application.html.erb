<!DOCTYPE html>
<html>
<head>
  <title>Boardprep2</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<script>
	$(function(){
		$('.pop').tooltip();
	});
</script>	

<div class="blue-head">
	<div class="container">
		<%= image_tag "logo.png" %>
	</div>
	<div class="light-blue-header">
		<div class='container'>
			<h1 class="pull-left hidden-xs">Board of Governors Exam</h1>
			<div class="pull-right">
				<button type="button" class="btn btn-default pop shuffle" data-toggle="tooltip" data-placement="left" title="Shuffle"><span class="glyphicon glyphicon-refresh"></span></button>
				<div class="btn-group" style="margin-right:25px;">
					<button type="button" class="btn btn-dark active">Color</button>
					<button type="button" class="btn btn-dark">Grey</button>
				</div>
				<div class="btn-group">
					<button type="button" class="btn btn-dark">All</button>
					<button type="button" class="btn btn-dark">Chairman</button>
					<button type="button" class="btn btn-dark">Board</button>
				</div>
			</div>
		</div>
	</div> 
</div>	
<div class="container">
	<%= yield %>
</div>

</body>
</html>
